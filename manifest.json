{
    "manifest_version": 3,
    "name": "SillyCase",
    "version": "1.0",
    "description": "A browser extension to convert the case of the text into a silly case.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": ["activeTab", "scripting"]
}
}
  